datasource postgres {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  address   String?
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     String[]
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model Todo {
  createdAt DateTime @default(now())
  demo      String?
  id        String   @id @default(cuid())
  test      String?
  title     String?
  updatedAt DateTime @updatedAt
}